var Range
Range = {
	"()" (from, to) => Range.clone {
		const from, to = from, to
	}
	
	iterate() => {
		var i = this.from-1
		const self = this
		return () => {
			i += 1
			return if i < self.to+1: i else nil
		}
	}
	
	contains(that) =>
		if that.is Number: that > this.from-1 and that < this.to+1
		else if that.is Range: that.from > this.from-1 and that.to < this.to+1
	
	toString() => "(" ++ this.from ++ ".." ++ this.to ++ ")"
}

yield Range
